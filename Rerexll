    exec.TextSize = 18
    exec.BackgroundColor3 = Color3.fromRGB(70,120,255)
    exec.TextColor3 = Color3.fromRGB(255,255,255)
    exec.BorderSizePixel = 0
    exec.Parent = item

    exec.MouseButton1Click:Connect(function()
        pcall(function()
            loadstring(data.code)()
        end)
    end)
end
